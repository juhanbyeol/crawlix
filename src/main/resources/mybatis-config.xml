<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    
    <!-- MyBatis Configuration -->
    
    <!-- SqlSessionTemplate related settings -->
    <settings>
        <setting name="jdbcTypeForNull" value="NULL" />
        <setting name="mapUnderscoreToCamelCase" value="true" />
        <setting name="cacheEnabled" value="true" />
        <setting name="defaultExecutorType" value="REUSE" />
    </settings>
    
    <!-- Type Aliases Section -->
    <typeAliases>
        <!-- Default type aliases -->
        <typeAlias type="java.util.HashMap" alias="hashmap" />
        <typeAlias type="java.util.List" alias="list" />
        <typeAlias type="java.lang.String" alias="string" />
        <typeAlias type="java.lang.Integer" alias="int" />
        
        <!-- Custom aliases for your project -->
  <!--       <typeAlias type="com.onestar.failfast.model.UserVo" alias="UserVo" />
        <typeAlias type="com.onestar.failfast.model.BoardVo" alias="BoardVo" /> -->
    </typeAliases>    
</configuration>
